
URxvt.font: xft:Inconsolata-12
URxvt.boldFont: xft:Inconsolata-12:bold
URxvt.italicFont: xft:Inconsolata-12:italic
URxvt.boldItalicFont: xft:Inconsolata-12:italic:bold
URxvt.letterSpace: -1
URxvt*lineSpace:              0               ! pixels between lines
URxvt*scrollTtyKeypress:      true            ! scroll to bottom on keypress
URxvt*scrollTtyOutput:        false           ! do not scroll to bottom on output
URxvt*buffered:               true            ! double buffer for xtf

URxvt.scrollstyle: plain
URxvt.scrollBar_right: true
URxvt.scrollColor: #3186AD
URxvt.thickness: 10
URxvt.saveLines: 5000
URxvt.perl-ext: default
URxvt.matcher.button: 1
URxvt.url-launcher: google-chrome
URxvt*iso14755: False

URxvt.termName: rxvt-unicode
URxvt.internalBorder: 0
URxvt.externalBorder: 0

URxvt.print-pipe: "cat > /dev/null"

