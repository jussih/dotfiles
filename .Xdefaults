URxvt.fading: 25
!URxvt.dynamicColors: on
!URxvt.intensityStyles: false

URxvt*background: #1B1B1B
URxvt*foreground: #DEDEDE
URxvt*fadeColor: #0a0a12

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC

URxvt.highlightColor: #3186AD
URxvt.highlightTextColor: white

URxvt.font: xft:Ubuntu Mono-12
URxvt.boldFont: xft:Ubuntu Mono-12:bold
URxvt.italicFont: xft:Ubuntu Mono-12:italic
URxvt.boldItalicFont: xft:Ubuntu Mono-12:italic:bold
!URxvt.letterSpace: -1

URxvt.scrollstyle: plain
URxvt.scrollBar_right: true
URxvt.scrollColor: #3186AD
URxvt.thickness: 10
URxvt.saveLines: 900
URxvt.perl-ext: default,matcher
URxvt.matcher.button: 1
URxvt.url-launcher: firefox

URxvt.termName: rxvt-unicode
URxvt.internalBorder: 0
URxvt.externalBorder: 0

